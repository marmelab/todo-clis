{
    "extends": [
        "eslint:recommended",
        "plugin:jest/recommended",
        "plugin:react/recommended",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "prettier",
        "jest",
        "react"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
        "env": {
        "browser": true,
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "warn",
        "react/prop-types": "warn"
    }
}